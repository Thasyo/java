public class Main {

	public static void main(String[] args) {
		

	    String s = "Thasyo Peres Leite";

	    String[] vect = s.toUpperCase().split(" ");

	    String word1 = vect[0];
	    String word2 = vect[1];
	    String word3 = vect[2];

	    System.out.println(word1);
	    System.out.println(word2);
	    System.out.println(word3);

	}
	

}
