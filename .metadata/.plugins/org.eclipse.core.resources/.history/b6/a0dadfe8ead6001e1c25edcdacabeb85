import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		double ray;
		double pi;
		double area;
		
		pi = 3.14159;
		ray = sc.nextDouble();
		area = pi * Math.sqrt(ray);
		
		System.out.println("A = " + area);
		
	}

}
